{
  "name": "My GitHub Dark Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0D1117",
    "editor.foreground": "#C9D1D9"
  },
  "tokenColors": [
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.method.declaration",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "meta.function-call.arguments",
        "constant.language"
      ],
      "settings": {
        "foreground": "#A5D6FF"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted.double",
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#A5D6FF"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8B949E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language",
        "storage.type.primitive",
        "storage.type.built-in",
        "storage.type.java",
        "variable.other.object.property.java",
        "support.variable.java"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "punctuation.definition.block",
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.definition.parameters",
        "punctuation.definition.method-parameters",
        "punctuation.parenthesis",
        "punctuation.curlybrace",
        "punctuation.brackets",
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    }
  ]
}
